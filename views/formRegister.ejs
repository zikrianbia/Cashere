<link href="//maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
<script src="//maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"></script>
<script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
<!------ Include the above in your HEAD tag ---------->
<%- include('partials/header')%>

<div class="sidenav">
         <div class="login-main-text p-3" >
            <h2>Cashere Register Page</h2>
            <p>Register from here to access.</p>
         </div>
      </div>
      <div class="main">
         <div class="col-md-6 col-sm-12">
            <div class="login-form">
               <form action="/kasirs/register" method="POST">
                  <div class="form-group">
                     <label for="name">Nama</label>
                     <input type="text" id="name" name="name" class="form-control" placeholder="Name" value="">
                  </div>
                  <div class="form-group">
                     <label>Email</label>
                     <input type="text" id="email" name="email" class="form-control" placeholder="Email" value="">
                  </div>
                  <div class="form-group">
                     <label>Password</label>
                     <input type="password" id="password" name="password" class="form-control" placeholder="Password">
                    </div>
                    <div class="container-fluid text-right mb-3">
                        <button type="submit" value="Submit" class="btn btn-success mb-3" >Sign Up</button>
                        <!-- <button type="button"  class="btn btn-secondary">Register</button> -->
                        <h6>Already have account?<a href="/kasirs">  Sign In !</a></h6>
                  </div>
                  
               </form>
            </div>
         </div>
      </div>

<%- include('partials/footer')%>