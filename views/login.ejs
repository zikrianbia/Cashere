<link href="//maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
<script src="//maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"></script>
<script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
<!------ Include the above in your HEAD tag ---------->
<%- include('partials/header')%>


<div class="sidenav">
    <div class="login-main-text p-3" >
       <h2>Cashere Login Page</h2>
       <p>Login or register from here to access.</p>
    </div>
 </div>
 <div class="main">
    <div class="col-md-6 col-sm-12 mb-3">
       <div class="login-form">
          <form action="/kasirs" method="POST">
             <div class="form-group">
                <label for="email">Email</label>
                <input type="text" id="email" name="email" class="form-control" placeholder="Email" value="">
             </div>
             <div class="form-group">
                <label for="password">Password</label>
                <input type="password" id="password" name="password" class="form-control" placeholder="Password" value="">
             </div>
             <div class="container-fluid text-right mb-4">
                 <input type="submit" value="Sign In" class="btn btn-success"></input>
                 <a href="/kasirs/register">Sign Up</a>
             </div>
             <!-- <button type="button"  class="btn btn-secondary">Register</button> -->
          </form>
       </div>
    </div>
 </div>